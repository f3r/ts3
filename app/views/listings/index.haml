.row
  .span12
    = breadcrumb [['Home', root_path]], t('products.my_listings')
  .span10.offset1
    .results.my_places
      - if @collection.blank?
        .alert-message.info= t("places.search.no_result")
      - else
        %ul.list-display
          #search_result_list
            = render :partial => 'search/row', :collection => @collection, :as => :resource, :locals => {:my_products => true}
