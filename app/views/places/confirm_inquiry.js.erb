$('#inquire_place').modal('hide');
$('.alert-message').remove();
<% if @response == "success" %>
$('.place_info').before('<div class="alert alert-success" data-alert="alert"><a href="#" class="close" data-dismiss="alert">×</a><%= t(:inquiry_success) %></div>');
<% else %>
$('.place_info').before('<div class="alert alert-error" data-alert="alert"><a href="#" class="close" data-dismiss="alert">×</a><%= t(:inquiry_error) %></div>');
<% end %>