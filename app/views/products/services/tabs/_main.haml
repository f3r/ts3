#main-tab.tab-pane.active
  .row
    .span3.profile_pic
      = image_tag @resource.primary_photo, :width => '200'
    .span8
      =# render 'products/custom_values'
      .row-fluid.about
        .span12
          %p= resource.description
      .row-fluid
        .span12
          = custom_field_image @resource, :facebook_url, 'social/facebook.png'
          = custom_field_image @resource, :twitter_url, 'social/twitter.png'
          = custom_field_image @resource, :myspace_url, 'social/myspace.png'
          = custom_field_image @resource, :soundcloud_url, 'social/soundcloud.png'
          = custom_field_image @resource, :youtube_url, 'social/youtube.png'
          = custom_field_image @resource, :rss_url, 'social/rss.png'