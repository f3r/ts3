.search-aside-wrapper
  .search-aside
    .panel-wrap
      %ul.panel-expander
        - if false
          %li
            = render 'button_alert'

        - category_filters = @search.category_filters
        - unless category_filters.empty?
          %li
            = render 'checkbox_filters', :filters => category_filters

        - amenity_filters = @search.amenity_filters
        - unless amenity_filters.empty?
          %li
            = render 'checkbox_filters', :filters => amenity_filters

        %li
          = render 'price_filters'