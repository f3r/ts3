%section.clearfix
  %h1 Search
  
  .row
    .span12
      .search-bar
        %form.form-stacked
          %fieldset.clearfix
            .clearfix
              .input.floatLeft
                %input{:class => 'span6', :id => 'search-key', :type => 'text', :placeholder => "What are you looking for?" }          
                %input{:class => 'btn primary', :value =>"Search", :type => 'submit' }          
              .loc-filter  
                %strong in Singapore
                %br            
                %a{:href => "#"} not where you are? 
        .toggle
          %a{:href => "#", :class => "disp-gallery"} 
          %a{:href => "#", :class => "disp-list current" }       
  
      .results        
        .header
          .title
            %h4 About 1,430 results found for "keyword"
          .controls
            %select{:class => "medium"}
              %option Newest First
              %option Option 2
              %option Option 3

        .alert-message.info No Results Found.          
        
        %ul.list-display
          %li
            .thumb.data-block
              %img{:src=>"http://placehold.it/114x74"}
            .name.data-block
              %h4.title
                %a{:href => "#"} Flat in Shangri-la towers Ortigas                
              .category
                %a{:href => "#"} Video Games &raquo;
                %a{:href => "#"} Consoles &raquo;
              %span.label.like_new Like New
            .flags.data-block
              .location Singapore, SG
              .condition
                %span.label.new New
                %span.label.nearby Nearby
            .figure.data-block
              .micro Buy it for
              .cost $198.83
            .people.data-block
              .details
                .name
                  %a{:href => "#"} Fer Martinez
                .rating
                  .smile 
                    %a{:href => "#"} 71
                  .star
                    %a{:href => "#"} 32
              .profile-pic
                %img{:src=>"http://placehold.it/40x40"}
              
              .time-stamp.micro 31 minutes ago  
              
            %li
              .thumb.data-block
                %img{:src=>"http://placehold.it/114x74"}
